{
  "name": "navitas-fitnes",
  "version": "1.0.0",
  "description": "none",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean --parallel build:** --parallel copy:*",
    "start": "npm-run-all clean --parallel build:scss webserver copy:* \"build:* -- --watch\"",
    "build:pug": "pug app -o ../src/NavitasFitness/webapp/ --pretty --hierarchy",
    "build:ts": "webpack",
    "build:scss": "node-sass styles/main.scss ../src/NavitasFitness/webapp/styles/main.css --source-map true --source-map-contents true",
    "creatWebappDir": "mkdir -p ../src/NavitasFitness/webapp",
    "copy:icons": "cp favicon.ico ../src/NavitasFitness/webapp/ && cp launcher.png ../src/NavitasFitness/webapp/",
    "copy:manifest": "cp manifest.json ../src/NavitasFitness/webapp/manifest.json",
    "copy:libs": "node buildenv/copyLibs.js",
    "copy:ui-grid-fonts": "mkdir -p ../src/NavitasFitness/webapp/styles && cp node_modules/angular-ui-grid/fonts/ui-grid.woff ../src/NavitasFitness/webapp/styles/",
    "test": "npm-run-all test:*",
    "test:tslint": "tslint --project .",
    "clean": "rm -rf ../src/NavitasFitness/webapp/* && npm run creatWebappDir",
    "webserver": "node buildenv/connect.server.js"
  },
  "author": "Stefan Krause-Kj√¶r",
  "license": "ISC",
  "devDependencies": {
    "@types/angular": "^1.6.50",
    "@types/angular-animate": "^1.5.9",
    "@types/angular-cookies": "^1.4.5",
    "@types/angular-material": "^1.1.57",
    "@types/angular-ui-router": "^1.1.40",
    "@types/ckeditor": "0.0.34",
    "@types/es6-shim": "^0.31.37",
    "@types/ui-grid": "0.0.40",
    "connect": "^3.6.0",
    "connect-modrewrite": "^0.9.0",
    "fs-extra": "^0.24.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^3.1.2",
    "pug": "^2.0.0-beta9",
    "pug-cli": "^1.0.0-alpha6",
    "serve-static": "^1.12.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "angular": "^1.6.7",
    "angular-animate": "^1.6.7",
    "angular-aria": "^1.6.7",
    "angular-cookies": "^1.6.7",
    "angular-material": "1.1.5",
    "angular-messages": "^1.6.7",
    "angular-mocks": "^1.6.7",
    "angular-sanitize": "^1.6.7",
    "angular-ui-grid": "^4.4.4",
    "angular-ui-router": "^0.4.2",
    "ckeditor": "^4.6.2",
    "es6-shim": "^0.35.3",
    "material-design-icons": "^2.2.3",
    "moment": "^2.17.1",
    "rxjs": "^5.5.5"
  }
}
