{
  "name": "navitas-fitnes",
  "version": "1.0.0",
  "description": "none",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean --parallel copy:* build:**",
    "watch": "npm-run-all clean --parallel build:scss webserver copy:* 'build:* -- --watch'",
    "build:jade": "jade app -o ../webapp/ --pretty --hierarchy",
    "build:ts": "tsc",
    "build:scss": "node-sass styles/main.scss ../webapp/main.css --source-map true",
    "copy:favicon": "cp favicon.ico ../webapp/favicon.ico",
    "clean": "rimraf ../webapp/*",
    "webserver": "node buildenv/connect.server.js",
    "install": "bower install && typings install && tsc"
  },
  "author": "Stefan Krause-Kj√¶r",
  "license": "ISC",
  "devDependencies": {
    "bower": "^1.7.9",
    "connect": "^3.4.1",
    "connect-modrewrite": "^0.8.2",
    "fs-extra": "^0.24.0",
    "jade": "^1.11.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^2.2.0",
    "serve-static": "^1.11.1",
    "typescript": "^2.0.2",
    "typings": "^1.0.4"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-cookies": "^1.5.8",
    "angular-material": "^1.1.0",
    "angular-messages": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "angular-ui-router": "^0.2.18",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.34.4",
    "material-design-icons": "^2.2.3",
    "rxjs": "^5.0.0-beta.11",
    "systemjs": "^0.19.37"
  }
}
