{
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "sourceRoot": "http://localhost:9000/src",
    "removeComments": false,
    "noImplicitAny": true,
    "noLib": false,
    "pretty": true,
    "listFiles": false,
    "rootDir": "./app/",
    "outDir": "./dist",
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "files": [
    "./app/NavitasFitnessModule.ts"
  ]
}